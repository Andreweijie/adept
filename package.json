    "concurrently": "^3.6.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.8",
    "mongoose-auto-increment": "^5.0.1",
    "multer": "^1.4.1",
    "node-otp": "^1.2.2",
    "node-sass": "^4.12.0",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-transition-group": "^4.2.1",
    "validator": "^10.11.0"
  }
